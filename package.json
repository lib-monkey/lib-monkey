{
  "name": "lib-monkey",
  "version": "0.4.1",
  "description": "Random Integration Testing for NodeJS",
  "main": "index.js",
  "bin": {
    "lib-monkey": "./bin/lib-monkey"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "build": "babel lib --out-dir dist",
    "prepublish": "npm test && npm run build",
    "test": "mocha --compilers js:babel-core/register",
    "cover": "cross-env NODE_ENV=test nyc mocha",
    "coveralls": "npm run cover && cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lib-monkey/lib-monkey.git"
  },
  "keywords": [
    "testing",
    "monkey",
    "integration",
    "random",
    "lib",
    "library",
    "cool"
  ],
  "author": "Team Of Monkeys",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lib-monkey/lib-monkey/issues"
  },
  "homepage": "https://github.com/lib-monkey/lib-monkey#readme",
  "maintainers": [
    {
      "name": "Dmitry Dodzin",
      "email": "d.dodzin@gmail.com"
    }
  ],
  "dependencies": {
    "chance": "^1.0.11",
    "colors": "^1.1.2",
    "commander": "^2.11.0",
    "throat": "^4.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^3.0.0",
    "cross-env": "^5.0.5",
    "mocha": "^3.5.3",
    "nyc": "^11.2.1",
    "sinon": "^4.0.0"
  }
}
