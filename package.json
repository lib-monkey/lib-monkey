{
  "name": "lib-monkey",
  "version": "0.6.0-beta-rev3",
  "description": "Random Integration Testing for NodeJS",
  "main": "index.js",
  "bin": {
    "lib-monkey": "./bin/lib-monkey"
  },
  "scripts": {
    "clean": "rm -rf dist docs",
    "prebuild": "yarn clean",
    "test": "mocha --require babel-core/register",
    "build": "yarn build-babel && yarn build-docs",
    "build-babel": "babel lib --out-dir dist",
    "build-docs": "jsdoc --readme DOCS.md -c .jsdocrc -d ./docs/ -r",
    "cover": "cross-env NODE_ENV=test nyc mocha",
    "coveralls": "yarn cover && cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lib-monkey/lib-monkey.git"
  },
  "keywords": [
    "testing",
    "monkey",
    "integration",
    "random",
    "lib",
    "library",
    "cool"
  ],
  "nyc": {
    "require": [
      "@babel/register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "author": "Team Of Monkeys",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lib-monkey/lib-monkey/issues"
  },
  "homepage": "https://github.com/lib-monkey/lib-monkey#readme",
  "maintainers": [
    {
      "name": "Dmitry Dodzin",
      "email": "d.dodzin@gmail.com"
    }
  ],
  "dependencies": {
    "chance": "^1.0.13",
    "colors": "^1.1.2",
    "commander": "^2.12.2",
    "throat": "^4.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/register": "^7.4.4",
    "babel-plugin-istanbul": "^5.1.4",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.3",
    "joi": "^14.3.1",
    "jsdoc": "^3.5.5",
    "mocha": "^6.1.4",
    "nyc": "^14.1.0",
    "sinon": "^7.3.2"
  }
}
